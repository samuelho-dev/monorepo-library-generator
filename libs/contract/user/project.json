{
  "name": "contract-user",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/contract/user/src",
  "tags": [
    "type:contract",
    "scope:user",
    "contract",
    "domain",
    "user",
    "builtin"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/contract/user",
        "tsConfig": "libs/contract/user/tsconfig.lib.json",
        "main": "libs/contract/user/src/index.ts",
        "batch": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/contract/user/src/**/*.ts"
        ]
      }
    }
  }
}
