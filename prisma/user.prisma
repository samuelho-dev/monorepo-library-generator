model user {
  /// @customType(Schema.String.pipe(Schema.brand('UserId')))
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email      String   @unique
  name       String
  username   String   @unique
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt

  @@index([email])
  @@schema("public")
}
