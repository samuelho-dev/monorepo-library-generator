generator client {
  provider = "prisma-client-js"
}

generator prisma_effect_kysely {
  provider     = "prisma-effect-kysely"
  output       = "../libs/types/database/src/lib"
  enumFileName = "enums.ts"
  fileName     = "types.ts"
}

datasource db {
  provider     = "postgresql"
  relationMode = "foreignKeys"
  schemas      = ["public"]
}
